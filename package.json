{
  "name": "agritrack-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed:admin": "ts-node --project tsconfig.seeder.json -r dotenv/config seed/seedAdmin.ts",
    "db:seed:produk": "ts-node --project tsconfig.seeder.json -r dotenv/config seed/seedProduk.ts",
    "db:seed:benih": "npm run db:seed:produk -- --only=benih",
